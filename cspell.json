  "flagWords": ["hte", "seperate", "indefinetly", "completly"],
  "dictionaries": ["rr-words", "cpp", "computing-acronyms", "software-tools", "softwareTerms", "en-gb"],
  "ignorePaths": ["node_modules", "public", ".github"]
}