{
  "version": "0.2",
  "language": "en",
  "dictionaryDefinitions": [
        {
            "name": "rr-words",
            "path": "./scripts/rr-spelling-dictionary.txt",
            "addWords": true
        }
    ],
  "ignoreRegExpList": [
    "`[^`]+`",
    "\\*\\*[^*]+\\*\\*",
    "\\[[^\\]]+\\]",
    "\"[A-Za-z0-9_]+\""
